        color: #00acd6;
    }
    .persist{
        display: block;
    }
    .attr-margin{
        margin-left: 159px;
    }

</style>

<script type="application/javascript">
    $( document ).ready(function() {
        $('#current-page').val(window.location.href);
        //-- determine the count of rating rows
        var ratingCount = $('#count-rating-status').val();
        function ratingDisplay(ratingCount){
            if(ratingCount == 1){
                $('.container-rating-2').hide();
                $('.container-rating-3').hide();
            } else if(ratingCount == 2){
                $('.container-rating-3').hide();
                $('.container-rating-2').show();
            } else if (ratingCount == 3){
                $('.container-rating-3').show();
                $('.container-rating-2').show();
            }
        }
        ratingDisplay(ratingCount);

        $('#rating-count').on('change', function(){
            ratingDisplay($(this).val());
        })
    })

</script>